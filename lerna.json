{
  "npmClient": "yarn",
  "npmClientArgs": ["--frozen-lockfile"],
  "useWorkspaces": true,
  "ignoreChanges": [
    "**/__fixtures__/**",
    "**/__tests__/**",
    "**/docs/**",
    "**/e2e/**",
    "**/examples/**",
    "**/*.md"
  ],
  "packages": ["config/*", "packages/*"],
  "version": "0.0.1"
}
